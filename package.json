{
  "name": "af-auth",
  "version": "1.1.0",
  "description": "Authentication service with GitHub OAuth and whitelist support",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/server.ts",
    "start": "node dist/server.js",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:migrate": "prisma migrate deploy",
    "db:migrate:dev": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "service-registry": "tsx scripts/manage-services.ts",
    "migrate:encrypt-tokens": "tsx scripts/migrate-encrypt-tokens.ts"
  },
  "keywords": [
    "auth",
    "github",
    "oauth",
    "typescript",
    "nodejs"
  ],
  "author": "Agent Foundry and John Brosnihan",
  "license": "Apache-2.0",
  "dependencies": {
    "@prisma/client": "5.22.0",
    "axios": "1.13.2",
    "bcrypt": "6.0.0",
    "dotenv": "16.4.5",
    "express": "4.22.1",
    "ioredis": "5.4.1",
    "jsonwebtoken": "^9.0.2",
    "pino": "9.5.0",
    "pino-http": "10.3.0",
    "pino-pretty": "11.3.0",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "4.17.21",
    "@types/jest": "29.5.14",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "22.9.3",
    "@types/react": "18.3.18",
    "@types/react-dom": "18.3.5",
    "@types/supertest": "6.0.2",
    "@typescript-eslint/eslint-plugin": "8.49.0",
    "@typescript-eslint/parser": "8.49.0",
    "eslint": "9.39.1",
    "globals": "^16.5.0",
    "jest": "29.7.0",
    "prisma": "5.22.0",
    "supertest": "7.1.3",
    "ts-jest": "29.4.6",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "typescript-eslint": "^8.49.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
