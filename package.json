{
  "name": "af-auth",
  "version": "1.0.0",
  "description": "Authentication service with GitHub OAuth and whitelist support",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/server.ts",
    "start": "node dist/server.js",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:migrate": "prisma migrate deploy",
    "db:migrate:dev": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio"
  },
  "keywords": [
    "auth",
    "github",
    "oauth",
    "typescript",
    "nodejs"
  ],
  "author": "Agent Foundry and John Brosnihan",
  "license": "Apache-2.0",
  "dependencies": {
    "@prisma/client": "5.22.0",
    "axios": "1.7.9",
    "dotenv": "16.4.5",
    "express": "4.21.1",
    "pino": "9.5.0",
    "pino-http": "10.3.0",
    "pino-pretty": "11.3.0",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@types/express": "4.17.21",
    "@types/jest": "29.5.14",
    "@types/node": "22.9.3",
    "@types/react": "18.3.18",
    "@types/react-dom": "18.3.5",
    "@types/supertest": "6.0.2",
    "@typescript-eslint/eslint-plugin": "8.15.0",
    "@typescript-eslint/parser": "8.15.0",
    "eslint": "8.57.1",
    "jest": "29.7.0",
    "prisma": "5.22.0",
    "supertest": "7.1.3",
    "ts-jest": "29.2.5",
    "tsx": "4.19.2",
    "typescript": "5.7.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
